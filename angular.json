{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false,
    "cache": {
            "enabled": true,
            "path": ".cache",
            "environment": "all"
        }
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "bills-main-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/bills-main-app",
      "sourceRoot": "projects/bills-main-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist/bills-main-app"
            },
            "index": "projects/bills-main-app/src/index.html",
            "polyfills": [
              "projects/bills-main-app/src/polyfills.ts"
            ],
            "tsConfig": "projects/bills-main-app/tsconfig.app.json",
            "assets": [
              "projects/bills-main-app/src/favicon.ico",
              "projects/bills-main-app/src/assets"
            ],
            "styles": [
              "projects/bills-main-app/src/styles.scss"
            ],
            "scripts": [],
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "browser": "projects/bills-main-app/src/main.ts"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/bills-main-app/src/environments/environment.ts",
                  "with": "projects/bills-main-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "bills-main-app:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "bills-main-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "bills-main-app:build"
          }
        }
      }
    },
    "views": {
      "projectType": "library",
      "root": "projects/views",
      "sourceRoot": "projects/views/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/views/tsconfig.lib.json",
            "project": "projects/views/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/views/tsconfig.lib.prod.json"
            }
          }
        }
      }
    },
    "tools": {
      "projectType": "library",
      "root": "projects/tools",
      "sourceRoot": "projects/tools/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/tools/tsconfig.lib.json",
            "project": "projects/tools/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/tools/tsconfig.lib.prod.json"
            }
          }
        }
      }
    },
    "model": {
      "projectType": "library",
      "root": "projects/model",
      "sourceRoot": "projects/model/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/model/tsconfig.lib.json",
            "project": "projects/model/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/model/tsconfig.lib.prod.json"
            }
          }
        }
      }
    },
    "store": {
      "projectType": "library",
      "root": "projects/store",
      "sourceRoot": "projects/store/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/store/tsconfig.lib.json",
            "project": "projects/store/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/store/tsconfig.lib.prod.json"
            }
          }
        }
      }
    },
    "bills-testing-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/bills-testing-app",
      "sourceRoot": "projects/bills-testing-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist/bills-testing-app"
            },
            "index": "projects/bills-testing-app/src/index.html",
            "polyfills": [
              "projects/bills-testing-app/src/polyfills.ts"
            ],
            "tsConfig": "projects/bills-testing-app/tsconfig.app.json",
            "assets": [
              "projects/bills-testing-app/src/favicon.ico",
              "projects/bills-testing-app/src/assets"
            ],
            "styles": [
              "projects/bills-testing-app/src/styles.scss"
            ],
            "scripts": [],
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "browser": "projects/bills-testing-app/src/main.ts"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/bills-testing-app/src/environments/environment.ts",
                  "with": "projects/bills-testing-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "bills-testing-app:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "bills-testing-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "bills-testing-app:build"
          }
        }
      }
    },
    "my-schematics": {
      "projectType": "library",
      "root": "projects/my-schematics",
      "sourceRoot": "projects/my-schematics/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/my-schematics/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/my-schematics/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/my-schematics/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
